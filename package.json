{
  "name": "@assetmantle/mantlejs",
  "version": "0.2.0",
  "description": "Client side JS libraries for AssetMantle SDK transaction generation, signing and broadcasting.",
  "main": "build/main.js",
  "types": "build/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AssetMantle/mantlejs.git"
  },
  "author": "Kombos <kamlesh@assetmantle.one>",
  "license": "MIT",
  "homepage": "https://github.com/AssetMantle/mantlejs#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6"
  },
  "scripts": {
    "watch": "babel src --out-dir lib --extensions \".ts,.tsx\" --watch",
    "build": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
